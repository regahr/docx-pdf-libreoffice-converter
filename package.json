{
  "name": "rest-service-template",
  "version": "1.0.0",
  "description": "Sprout Asia code template for REST API service",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=dev nodemon --watch ./app --delay 500ms --exec babel-node ./app/app.js",
    "dev:windows": "set \"NODE_ENV=dev\" &&  nodemon --watch ./app --delay 500ms --exec babel-node ./app/app.js",
    "test": "NODE_ENV=test mocha",
    "test:windows": "set \"NODE_ENV=test\" && mocha",
    "test:coverage": "rm -rf coverage && nyc --reporter=text --reporter=lcov mocha",
    "lint": "echo 'Running lint...' && eslint app/",
    "lint:fix": "eslint app/ --fix",
    "prepush": "npm run lint && npm run test:windows",
    "build-server": "babel -d ./build ./app -s",
    "start": "node ./build/app.js",
    "run-script:migrate-recipient": "NODE_ENV=dev babel-node ./app/scripts/recipient-migration.js",
    "run-script:change-image-country": "NODE_ENV=dev babel-node ./app/scripts/system-variable-country-image-changer.js",
    "run-script:refresh-countries": "NODE_ENV=dev babel-node ./app/scripts/system-variable-refresh-countries.js",
    "run-script:coa-new-version": "NODE_ENV=dev babel-node ./app/scripts/coa-template-migration.js",
    "run-script:generate-user-email": "NODE_ENV=dev babel-node ./app/scripts/generate-email-bill-recipient.js",
    "run-script:sweep-remit-bank-fee": "NODE_ENV=dev babel-node ./app/scripts/sweep-remit-bank-fee-on-swift-trx.js",
    "run-script:init-roles": "NODE_ENV=dev babel-node ./app/scripts/init-roles.js",
    "run-script:sysvar-rwc-migration": "NODE_ENV=dev babel-node ./app/scripts/system-variable-rwc-migration.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SproutAsia/sprout-finance-api.git"
  },
  "keywords": [
    "sprout-asia",
    "finance"
  ],
  "author": "Dev Team Sprout",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SproutAsia/sprout-finance-api/issues"
  },
  "homepage": "https://github.com/SproutAsia/sprout-finance-api#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "amazon-cognito-identity-js": "^5.2.8",
    "@aws-sdk/client-s3": "^3.43.0",
    "analytics-node": "^6.2.0",
    "axios": "^0.23.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "ion-js": "^4.3.0",
    "joi": "^17.4.2",
    "jsbi": "^4.3.0",
    "jsonwebtoken": "^8.5.1",
    "level": "^8.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.42",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "nyc": "^15.1.0",
    "rotating-file-stream": "^1.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-object-assign": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "debug": "^4.3.2",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-unused-imports": "^1.1.5",
    "husky": "^7.0.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.14",
    "sinon": "^11.1.2"
  }
}
